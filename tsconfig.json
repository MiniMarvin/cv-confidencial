        "module": "CommonJS",
        "target": "ES2020",
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "outDir": "fileService/build",
        "sourceMap": true
    },
    "include": [
        "fileService/src/**/*",
        "fileService/tests/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "pdfparse"
    ]
}